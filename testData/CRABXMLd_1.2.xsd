		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="gml:AbstractFeatureType">
					<xs:sequence>
						<xs:element name="COMPONENTEN" minOccurs="1" maxOccurs="1">
							<xs:complexType>
								<xs:all>
									<xs:element name="STRAATNAMEN">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="STRAATNAAM_OBJECT" minOccurs="0" maxOccurs="unbounded">
													<xs:complexType>
														<xs:sequence>
														<xs:annotation>
															<xs:documentation>voorlopige straatcodes moeten groter of gelijk zijn aan 1E+9</xs:documentation>
														</xs:annotation>
															<xs:element name="ID" type="xs:int"/>
															<xs:element name="STRAATCODE" type="c:crabstraatcodetype"/>
															<xs:element name="NISGEMEENTECODE" type="c:nisgemeentecodetype"/>
															<xs:element name="STRAATNAAM">
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:minLength value="1"/>

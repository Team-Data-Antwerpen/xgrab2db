	<xs:simpleType name="nisgemeentecodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-9][0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="crabstraatcodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,15}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="postkantoncodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-9][0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="subkantoncodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-9][0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="rrstraatcodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="(T|K|[0-9])[0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="rrhuisnummertype">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="11"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="rrindextype">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kadstraatcodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{5}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="kadhuisnummertype">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="11"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="huisnummertype">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:pattern value="[1-9][0-9]*(BIS|TER|[A-Z]|_[0-9]+){0,1}"/>
		</xs:restriction>
	</xs:simpleType>

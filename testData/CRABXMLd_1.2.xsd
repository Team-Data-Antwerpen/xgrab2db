																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="TAALCODESTRAATNAAM" type="c:taalcode"/>
															<xs:element name="TWEEDETAAL" minOccurs="0">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="STRAATNAAMTWEEDETAAL">
																			<xs:simpleType>
																				<xs:restriction base="xs:string">
																					<xs:minLength value="1"/>
																					<xs:maxLength value="80"/>
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																		<xs:element name="TAALCODESTRAATNAAMTWEEDETAAL" type="c:taalcode"/>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>

										</xs:complexType>
										<xs:key name="PK_straatnaam">
											<xs:selector xpath="./c:STRAATNAAM_OBJECT"/>
											<xs:field xpath="c:ID"/>
											<xs:field xpath="c:BEGINMETADATA/c:TIJD"/>
										</xs:key>																
										<xs:unique name="UK_straatnaam">
											<xs:selector xpath="c:STRAATNAAM_OBJECT"/>
											<xs:field xpath="c:NISGEMEENTECODE"/>
											<xs:field xpath="c:STRAATNAAM"/>
											<xs:field xpath="c:BEGINDATUM"/>
											<xs:field xpath="c:BEGINMETADATA/c:TIJD"/>
										</xs:unique>
										<xs:unique name="UK2_straatnaam">
											<xs:selector xpath="c:STRAATNAAM_OBJECT"/>
											<xs:field xpath="c:STRAATCODE"/>
											<xs:field xpath="c:BEGINMETADATA/c:TIJD"/>
										</xs:unique>
									</xs:element>
									<xs:element name="STRAATNAAMSTATUSSEN">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="STRAATNAAMSTATUS_OBJECT" minOccurs="0" maxOccurs="unbounded">
